@model List<UserWithRoleView>
@{
    ViewData["Title"] = "Index";
}



<h1 class="custom-hover text-center">Brukeroversikt</h1>
<a class="btn btn-primary mb-2" asp-area="Identity" asp-page="/Account/Register">Opprett ny bruker</a>
<table class="table table-bordered border-dark table-responsive bg-opacity-75 bg-white rounded-3">
    <thead class="table-dark">
    <tr class="">
        <th style="width: 20%;">Bruker ID</th>
        <th>Brukernavn</th>
        <th>Email</th>
        <th>Rolle</th>
        <th style="width: 20%;">Handlinger</th>

    </tr>
    </thead>
    <tbody>
    @foreach (var userWithRole in Model)
    {
        <tr>
            <td>@userWithRole.User.Id</td>
            <td>@userWithRole.User.UserName</td>
            <td>@userWithRole.User.Email</td>
            <td>@userWithRole.Role.Name</td>
            <td>
                <div class="d-grid gap-2">
                    <a class="btn btn-warning col" asp-action="Edit" asp-route-id="@userWithRole.User.Id">Rediger</a>
                    <a class="btn btn-info col" asp-action="Details" asp-route-id="@userWithRole.User.Id">Detaljer</a>
                    <a class="btn btn-danger col" asp-action="Delete" asp-route-id="@userWithRole.User.Id">Slett</a>
                </div>
                </td>
        </tr>
    }
    </tbody>
</table>
